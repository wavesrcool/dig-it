  "contributors": [],
  "description": "",
  "engines": {
    "node": ">=14.0.0 <17.0.0"
  },
  "license": "MIT",
  "name": "@dig-it/dev",
  "repository": {
    "directory": "packages/dev",
    "type": "git",
    "url": "https://github.com/wavesrcool/dig-it.git"
  },
  "type": "commonjs",
  "version": "0.1.3",
  "bin": {
    "dig-it-files-rename": "lib/bin/files-rename/main.js",
    "dig-it-ci-env-example": "lib/bin/ci-env-example/main.js"
  },
  "files": [
    "./lib"
  ],
  "keywords": [
    "dig",
    "it",
    "dev",
    "es6",
    "node"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "ci": "yarn lint",
    "w": "tsc -w",
    "0l": "rm -rf lib",
    "0t": "rm -f tsconfig*tsbuildinfo",
    "c": "yarn 0l && yarn 0t && tsc --project tsconfig.build.json",
    "lint": "eslint './src/**/*.{ts,tsx}' --max-warnings=0 --fix",
    "build": "yarn ci && yarn c && yarn executable-bins",
    "build-dev": "yarn lint && yarn c && yarn executable-bins",
    "prepack": "yarn build",
    "test": "yarn jest",
    "executable-bins": "chmod u+x ./lib/bin/**/main.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/jsonfile": "^6.1.0",
    "@types/lodash": "^4.14.185",
    "@types/yargs": "^17.0.12"
  },
  "dependencies": {
    "@dig-it/env": "^0.1.3",
    "glob": "^8.0.3",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "yargs": "^17.5.1"
  },
  "gitHead": "e3caba927b044cb9c72238dfbb7744ac1ae8c757"
}
